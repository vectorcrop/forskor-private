// Sweet Alert
Swal.fire({
    toast: true,
    icon: 'success',
    customClass: 'my',
    title: response.message,
    animation: true,
    position: 'top-right',
    showConfirmButton: false,
    timer: 3000,
    iconColor:"#fff",
    timerProgressBar: true,
    didOpen: (toast) => {
      toast.addEventListener('mouseenter', Swal.stopTimer)
      toast.addEventListener('mouseleave', Swal.resumeTimer)
    }
  })